body
	padding 0px
	margin 0px
	margin-top: -20px;
	position relative
	font-family: Open Sans, Helvetica, Arial, sans-serif;
	letter-spacing 1px
	vertical-align baseline
	-webkit-font-smoothing: antialiased;

.dynamic-content
	padding-top: 110px;
	min-height: 600px;

#shadow
	position absolute
	top 0
	left 0
	right 0
	bottom 0
	background #525252
	opacity 0.5
	z-index 10000
	display none

#form-box
	position fixed
	width: 460px;
	height: 458px;
	background-color #70CFDF
	z-index 100000
	display none
	top: 50%;
	margin-top: -229px;
	left: 50%;
	margin-left: -230px;
	border-radius: 4px;

#form-box .jumbotron
	width: 460px;
	height: 458px;
	position: absolute;
	left: 50%;
	margin-left: -230px;
	top: 50%;
	margin-top: -229px;

#form-box .jumbotron form
	margin-top: 25px;

#form-box .jumbotron img
	width: 27px;
	position: absolute;
	right: 14px;
	top: 14px;

form input.form-control
	margin-bottom 10px
	width 95%
	border: solid 2px #B8B4B4;

form textarea.form-control
	width 95%
	border: solid 2px #B8B4B4;

#form-box form button
	width 120px
	font-size 14px
	height 32px
	margin-bottom 0px
	margin-top: 30px;
	background-color: #F79226;
	border-color: #F7A946;
	
.loading
	position: absolute;
	display none
	width: 180px;
	height: 180px;
	background: url("https://www.findshare.com/assets/images/loading-blue.gif");
	background-size: contain;
	top: 50%;
	left: 50%;
	margin-top: -90px;
	margin-left: -90px;
	z-index: 999;

a, a:hover
	text-decoration none

h2
	font-size: 22px;
	color: #4682B4;
	font-weight: bold;

h3
	font-size: 20px;
	color: #4682b4;

ul
	padding-left 0

li
	list-style none
	color: #4682b4;
	margin-bottom: 4px;

header
	background-color: #FFF
	height 110px
	border-bottom: solid 1px #E0E8EC

.center
	margin 0 auto
	width 1024px

.jumbotron
	padding 25px
	background transparent

.jumbotron p
	color: #4682B4;;
	font-weight: bold;
	margin-bottom: 25px;

.jumbotron ul
	padding-left: 20px;
	margin-bottom: 35px;

.jumbotron ul li
	margin-bottom: 5px;
	color: #665F5F;
	list-style: none;
	height: 45px;

.jumbotron ul li span
	float: right;
	width: 315px;

.jumbotron span
	color: #665F5F;

.jumbotron div a
	display: block;
	width: 160px;
	overflow: hidden;
	float left
	margin-right: 25px;
	margin-top: 20px;

.jumbotron div a img
	width 100%

#logo-box
	width: 195px;
	height: 110px;
	float: left;
	padding-left: 20px;

#logo-box a
	width: 100%;
	display: block;
	margin-top: 40px;

#logo-box img
	width 100%

#main-header
	height: 90px;
	float: right;
	margin-bottom: 0;
	margin-top: 20px;

#main-header li
	float: left;
	list-style: none;
	line-height: 90px;

li.vertical-divider
	height: 90px;
	margin: 0 10px;
	border-left: 1px solid #e0e8ec;
	border-right: 1px solid #fff;

#main-header li a
	padding: 10px 20px;
	font-weight: bold;
	
#main-header li a:hover
	font-size 15px
	color #F8580D

#main-header li a span
	margin-right 12px

#main_container
	background-color: #C7DFEC;
	border-bottom: solid 1px #B8CBD4;

#main_container .center
	padding: 30px 0;

#main_container .center #main-box
	height 610px
	margin-top: 50px;
	padding: 0 20px;
	margin-bottom: 20px;

.center #main-box .mb-left
	width 530px
	/*height: 100%;*/
	float right

.center #main-box .mb-right
	width: 420px;
	float: left;

#main-box .mb-right p
	margin-bottom 45px
	
#main-box .mb-right ul li
	margin-bottom 25px

#main-box .mb-left #mb-form-box
	height: 100%;
	background: steelblue;
	border-radius: 14px;
	padding: 25px 60px;

.mb-left #mb-form-box p
	font-size: 23px;
	color: white;

#mb-form-box form
	margin-top: 15px;

.mb-left #mb-form-box label
	display: block;
	color: white;
	font-size: 18px;
	font-weight: normal;

.mb-left #mb-form-box select
	margin-bottom: 25px;
	font-size: 18px;

.btn-block	
	margin-top: 35px;
	font-weight: bold;
	
.btn-block#search_doctor_main
	margin-top 20px	

#providers_container
	background-color: #F8F8F8;
	border-top: solid 1px #e2e2e2;
	
#info_container
	background-color: white;
	
#info_container .center
	padding: 30px 0;

#info_container .center .info-box
	height: 250px;
	padding: 0 20px;
	margin-bottom: 20px;
	
#info_container .column
	text-align center
	
#info_container .column img
	margin-bottom 35px
	
#info_container .column h4
	color #4682B4
	
#info_container .column .orange-text
	color #F8580D

#providers_container .center
	padding: 30px 0;

#providers_container .center .info-box
	height: 425px;
	padding: 0 20px;
	margin-bottom: 20px;

.info-box .inner-info-box .column
	float: left;
	width: 33.33%;

.inner-info-box .column hr 
	border-top: solid 1px #EC6526;
	width: 70%;
	float: left;
	padding-right: 80px;
	margin: 10px 0 20px;


#docs_container
	background-color: white;
	border-top: solid 1px #E2E2E2;

#docs_container .center
	padding: 30px 0;

#docs_container .center h2, #docs_container .center p
	color #665f5f

#docs_container .center p
	font-size: 17px;
	padding-right: 50px;

#docs_container .center a
	width: 250px;
	height: 36px;
	line-height: 14px;
	margin-top: 25px;
	font-weight bold	

#docs_container .center .info-box
	height: 200px;
	padding: 0 20px;
	margin-bottom: 20px;

.info-box .inner-info-box .column-b
	float: left;
	width: 50%;

.info-box .inner-info-box .column-b:first-child
	border-right: dotted 1px #665f5f;

.info-box .inner-info-box .column-b:last-child
	padding-left: 60px;

.p-fix
	float: left;
	margin-right: 8px;
	
#sign-in-box .inner-info-box .form-box.box-fix
	margin 0
	float left
	
#sign-in-box .inner-info-box .form-box.box-fix:first-child
	margin-right 50px
	
#sign-in-box .inner-info-box .form-box.box-fix:last-child
	float right

footer
	height: 320px;
	background-color: #4682b4;
	border-top: solid 1px #2980B9;
	position relative
	z-index 20

footer .center
	padding 30px 0
	padding-bottom: 10px;

footer .center .info-box
	padding: 0 20px;
	margin-bottom: 20px;

footer .column
	float: left;
	width: 33.33%;

footer .column:last-child
	text-align center

footer h2
	color: #fff;
	margin-top: 0;
	margin-bottom: 20px;
	font-weight: normal;
	font-size: 18px;

footer i
	color white

footer .column span
	margin-left: 5px;
	color: white;
	font-size: 18px;

footer .column  button
	padding-left: 35px;
	padding-right: 35px;

footer .column-c
	width: 25%;
	float: left;

footer .column-c ul
	margin-top 30px

footer .column-c b,footer .column-c a
	color white

footer .column-c i
	margin-right 6px

footer .center .info-box.last-box
	text-align: center;
	color: white;
	font-size: 12px;
	margin-bottom 0

footer .center .info-box.last-box a
	color white

footer .center .info-box.last-box a:hover
	text-decoration underline
	
footer .footer-top
	width: 100%;
	background: #3498DB;
	height: 140px;
	
footer .footer-bottom
	width: 100%;
	background: #2980B9;