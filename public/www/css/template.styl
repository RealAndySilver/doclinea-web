body
	padding 0px
	margin 0px
	margin-top: -20px;
	position relative
	font-family: Arial,Helvetica,sans-serif;
	letter-spacing 1px
	vertical-align baseline

a, a:hover
	text-decoration none

h2
	font-size: 22px;
	color: #4682B4;
	font-weight: bold;

header
	background-color: #ECF3F8;
	height 110px
	border-bottom: solid 1px #E0E8EC;

.center
	margin 0 auto
	width 1024px

.jumbotron
	padding 25px
	background transparent

.jumbotron p
	color: #4682B4;;
	font-weight: bold;
	margin-bottom: 25px;

.jumbotron ul
	padding-left: 20px;
	margin-bottom: 35px;

.jumbotron ul li
	margin-bottom: 5px;
	color: #665F5F;
	list-style: none;
	height: 45px;

.jumbotron ul li span
	float: right;
	width: 315px;

.jumbotron span
	color: #665F5F;

.jumbotron div a
	display: block;
	width: 160px;
	overflow: hidden;
	float left
	margin-right: 25px;
	margin-top: 20px;

.jumbotron div a img
	width 100%

#logo-box
	width: 320px;
	height: 110px;
	float: left;
	padding-left: 20px;

#logo-box img
	height 100%

#main-header
	height: 90px;
	float: right;
	margin-bottom: 0;
	margin-top: 20px;

#main-header li
	float: left;
	list-style: none;
	line-height: 90px;

li.vertical-divider
	height: 90px;
	margin: 0 10px;
	border-left: 1px solid #e0e8ec;
	border-right: 1px solid #fff;

#main-header li a
	padding: 10px 20px;

#main_container
	background-color: #CDDCE4;
	border-bottom: solid 1px #B8CBD4;

#main_container .center
	padding: 30px 0;

#main_container .center #main-box
	height 480px
	margin-top: 50px;
	padding: 0 20px;
	margin-bottom: 20px;

.center #main-box .mb-left
	width 530px
	height: 100%;
	float right

.center #main-box .mb-right
	width: 420px;
	float: left;

#main-box .mb-left #mb-form-box
	height: 100%;
	background: steelblue;
	border-radius: 14px;
	padding: 25px 60px;

.mb-left #mb-form-box p
	font-size: 23px;
	color: white;

#mb-form-box form
	margin-top: 35px;

.mb-left #mb-form-box label
	display: block;
	color: white;
	font-size: 18px;
	font-weight: normal;

.mb-left #mb-form-box select
	margin-bottom: 25px;

.btn-block	
	margin-top: 35px;

#providers_container
	background: #F8F8F8;

#providers_container .center
	padding: 30px 0;

#providers_container .center #info-box
	height 480px
	padding: 0 20px;
	margin-bottom: 20px;
